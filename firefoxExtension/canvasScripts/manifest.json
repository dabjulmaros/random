{
  "manifest_version": 2,
  "name": "Canvas Scripts",
  "version": "0.1",
  "description": "Unofficial scritps for Instructure Canvas.",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://fsw.instructure.com/*"
      ],
      "js": [
        "scripts.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "myCustomScripts@dabjulmaros.io"
    }
  }
}