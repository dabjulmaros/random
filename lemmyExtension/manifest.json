{
  "manifest_version": 3,
  "name": "Find@home",
  "version": "1.0",
  "description": "Runs basic script find posts in home instance",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://*/*"]
    }
  ],
  "action": {
    "default_popup": "action/popup.html"
  },
  "permissions":["storage"]
}
